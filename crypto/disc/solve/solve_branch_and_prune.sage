n = 

from Crypto.Util.number import *
words = {w[i:] for w in open('words_alpha.txt').read().split() for i in range(len(w))}
chars = set(b' abcdefghijklmnopqrstuvwxyz')

def good(bs):
    return set() == set(bs) - chars == set(bs.decode().split()) - words

def test(bs):
    for i in chars:
        j = bytes_to_long(tmp := bytes([i]) + bs)
        if n%j==0:
            print(tmp)
            
        if j%2 and good(tmp) and good(long_to_bytes(n / j % 256^len(tmp))):
            test(tmp)

test(b'')